{
    "name": "analogue/laravel-auth",
    "description": "AnalogueORM Authentication Driver for Laravel 5",
    "homepage": "http://github.com/analogueorm/laravel-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "RÃ©mi Collin",
            "email": "remi@code16.fr"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.5.*|5.6.*",
        "analogue/orm": "5.5.*|5.6.*"
    },
    "require-dev": {
        "laravel/laravel": "5.5.*|5.6.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^5.0",
        "symfony/css-selector": "2.8.*|3.0.*|4.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*|4.0.*",
        "laravel/browser-kit-testing": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Analogue\\LaravelAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Analogue\\LaravelAuth\\AnalogueAuthServiceProvider"
            ]
        }
    }
}
